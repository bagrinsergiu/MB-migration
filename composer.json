{
  "name": "bagrinsergiu/mb-migration",
  "description": "Library for migration from Ministry Brands to Brizy",
  "type": "library",
  "require": {
    "php": "7.4.*",
    "gmostafa/php-graphql-client": "^1.13",
    "ext-dom": "*",
    "ext-pdo": "*",
    "ext-fileinfo": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "leafo/scssphp": "^0.8.4",
    "scssphp/scssphp": "^1.11",
    "zoon/puphpeteer": "1.6.0",
    "monolog/monolog": "^1.23",
    "chrome-php/chrome": "^1.10",
    "symfony/dotenv": "^5.4",
    "symfony/runtime": "^5.4",
    "symfony/http-foundation": "^5.4"
  },
  "autoload": {
    "psr-0": {
      "MBMigration": "lib/"
    }
  },
  "scripts": {
    "post-install-cmd": "npm install && npm run build:prod"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/golovencoserghei/rialto.git"
    }
  ],
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
