{
  "name": "bagrinsergiu/mb-migration",
  "description": "Library for migration from Ministry Brands to Brizy",
  "type": "library",
  "require": {
    "php": "7.4.*",
    "gmostafa/php-graphql-client": "^1.13",
    "ext-dom": "*",
    "ext-pdo": "*",
    "ext-fileinfo": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "leafo/scssphp": "^0.8.4",
    "scssphp/scssphp": "^1.11",
    "monolog/monolog": "^1.23",
    "chrome-php/chrome": "^1.10",
    "symfony/dotenv": "^5.4",
    "symfony/runtime": "^5.4",
    "symfony/http-foundation": "^5.4",
    "ext-gd": "*",
    "aws/aws-sdk-php": "^3.337",
    "robmorgan/phinx": "^0.14.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-0": {
      "MBMigration": "lib/",
      "BRector": "rector/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Utils\\Rector\\": "utils/rector/src"
    }
  },
  "scripts": {
    "post-install-cmd": "npm install && npm run build:prod",
    "test": "vendor/bin/phpunit -c phpunit.xml.dist",
    "test:all": "vendor/bin/phpunit -c phpunit.xml.dist",
    "test:coverage": "vendor/bin/phpunit -c phpunit.xml.dist --coverage-html coverage"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "rector/rector": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
