{
  "private": true,
  "name": "mb-migration",
  "version": "1.0.0",
  "workspaces": [
    "packages/*",
    "lib/MBMigration/Builder/Layout/Theme/Voyage/assets",
    "lib/MBMigration/Builder/Layout/Theme/Anthem/assets"
  ],
  "scripts": {
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "tsc": "turbo run tsc",
    "prettier-check": "turbo run prettier-check",
    "check-all": "npm run prettier-check && npm run lint && npm run tsc"
  },
  "devDependencies": {
    "turbo": "^1.10.16"
  }
}
